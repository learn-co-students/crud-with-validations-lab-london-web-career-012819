
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Release Year</th>
      <th>Genre</th>
      <th>Artist Name</th>
    </tr>
  </thead>



<tbody>
<% @songs.each do |song| %>
<tr>


<td><%= song.title %></td>
<td><%= song.release_year %></td>
<td><%= song.artist_name %></td>
<td><%= song.genre %></td>
<td>
  <%= link_to 'View', song %> |
  <%= link_to 'Deit', edit_song_path(song) %> |
  <%= link_to 'Delete', song, method: :delete, data: { confirm: "Really?" } %>

</td>
</tr>
<% end %>
</tbody>
</table>
